    <record id="view_move_kandan_custome" model="ir.ui.view">
        <field name="name">stock.move.kanban</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_kandan"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@class='o_kanban_record_headings']//field[@name='product_id']" position="after">
                <div style="padding-left:160px;">
                    <field name="is_track" invisible="1"/>
                    <button name="start_scan" attrs="{'invisible':['|',('state', '!=','assigned'),('is_track','=',True)]}" value="Start Scan" type="object" class="btn btn-primary">Start Scan</button>
                </div>

            </xpath>
        </field>
    </record>



