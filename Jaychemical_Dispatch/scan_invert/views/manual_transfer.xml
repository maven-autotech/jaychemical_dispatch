<odoo>
    <template id="manual_transfer_page" name="Inward">
<!--        <script type="text/javascript" src="scan_invert/static/js/focus.js"/>-->
<!--        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js"/>-->
<!--        <t t-call="website.layout">-->
            <div class="oe_structure">
                <div class="container">
                    <br/>


                    <!-- <form action="/back_home" method="post" autocomplete="off">
                        <div class="form-group row">
                            <div class="col-lg-12 col-md-12 text-cencter">
                                <input type="submit" class="btn btn-outline-danger btn-lg" style="width:120px;height:40px;valign:middle;" value="Back" />
                            </div>
                        </div>
                    </form> -->

                    <audio id="myAudio">
                     <source src="scan_invert/static/audio/buzzer.mp3" type="audio/mp3"/>
                </audio>
                    <audio id="myAudio1">
                     <source src="scan_invert/static/audio/warning.mp3" type="audio/mp3"/>
                </audio>
                    <center>
                        <table style="font-size: 12px;">
                            <tr>
                                <td>Required(Total) Quantity</td>
                                <td></td>
                                <td>:</td>
                                <td></td>
                                <td>
                                    <b>
                                        <t t-esc="qty_reqired" />
                                    </b>
                                </td>
                            </tr>

                            <tr>
                                <td>Done(Scanned) Quantity</td>
                                <td></td>
                                <td>:</td>
                                <td></td>
                                <td>
                                    <t t-esc="qty_done" />
                                </td>
                            </tr>
                        </table>
                        
                        <div id="wrap" class="oe_structure" style="overflow-y:auto; height:170px;">
                            <t t-if="table1">
                                <table class="table table-bordered table-condensed" style="border-collapse:collapse; width:90%; font-size: 12px;">
                                    <thead style="position:sticky; top:0; border:2px solid">
                                        <th style="background:#C0C0C0; position:sticky; border:2px solid; width:25%;">
                                            <b>Material</b>
                                        </th>
                                        <th style="background:#C0C0C0; position:sticky; border:2px solid; ">
                                            <b>Rec Qty</b>
                                        </th>
                                        <th style="background:#C0C0C0; position:sticky; border:2px solid; ">
                                            <b>Scn Qty</b>
                                        </th>
                                        <th style="background:#C0C0C0; position:sticky; border:2px solid; width:35%">
                                            <b>Location</b>
                                        </th>
                                    </thead>
                                    <t t-foreach="table1" t-as="item">
                                        <tbody>
                                            <tr>
                                                <td style="border:2px solid">
                                                    <t t-esc="item" />
                                                </td>
                                                <td style="border:2px solid">
                                                    <t t-esc="item_value[0]" />
                                                </td>
                                                <td style="border:2px solid">
                                                    <t t-esc="item_value[1]" />
                                                </td>
                                                <td style="border:2px solid">
                                                    <t t-esc="item_value[2]" />
                                                </td>
                                            </tr>
                                        </tbody>
                                    </t>
                                </table>
                            </t>

                            <t t-if="table2">
                                <table class="table table-bordered table-condensed" style="border-collapse:collapse; width:90%; font-size: 12px;">
                                    <thead style="position:sticky; top:0; border:2px solid">
                                        <th style="background:#C0C0C0; position:sticky; border:2px solid; width:25%;">
                                            <b>Product</b>
                                        </th>
                                        <th style="background:#C0C0C0; position:sticky; border:2px solid; ">
                                            <b>Total Quantity</b>
                                        </th>
                                        <th style="background:#C0C0C0; position:sticky; border:2px solid; ">
                                            <b>Remain Quantity</b>
                                        </th>
                                    </thead>
                                    <t t-foreach="table2" t-as="item">
                                        <tbody>
                                            <tr>
                                                <td style="border:2px solid">
                                                    <t t-esc="item" />
                                                </td>
                                                <td style="border:2px solid">
                                                    <t t-esc="item_value[0]" />
                                                </td>
                                                <td style="border:2px solid">
                                                    <t t-esc="item_value[1]" />
                                                </td>
                                            </tr>
                                        </tbody>
                                    </t>
                                </table>
                            </t>
                        </div>

<!--                        <h3 t-esc="error" class="text-error" style="color:red" />-->
<!--                        <h3 t-esc="success" class="text-success" />-->
<!--                        <h3 t-esc="name" style="color:blue" />-->
                        
                        <h1 style="margin-bottom:30px;margin-top:50px">
                            Scan Label
                            <h3 t-esc="pro_track" class="text-danger" />
                        </h1>
                        <form action="/scan_manual_transfer" method="post" autocomplete="off">
                            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()" />
                            <div class="form-group row form-field o_website_form_custom o_website_form_required_custom">
                                <div class="col-lg-3 col-md-4">
                                    <label class="col-form-label" for="serial">Scan label:</label>
                                </div>
                                <div class="col-lg-7 col-sm-8">
                                    <input onblur="input1(event)" autofocus="True" type="text" id="serial" name="serial" autocomplete="off" required="" class="form-control form-control-lg o_website_form_input" />
                                </div>
                            </div>
                            <div class="form-group row">
                                <div class="col-lg-12 col-md-12 text-cencter">
                                    <input type="submit" class="btn btn-outline-primary btn-lg" />
                                </div>
                            </div>
                        </form>
                        <h3 t-esc="success" class="text-success" />
                       <div>
                        <t t-if="error">
                        <audio id="myAudio">
                        <source src="scan_invert/static/audio/buzzer.mp3" type="audio/mp3"/>
                        </audio>
                        <div id="error_audio">
                        <h3 t-esc="error" class="text-error" style="color:red"/>
                        </div>
                        <script> var divs = document.getElementById("error_audio"); if (divs) { document.getElementById("myAudio").play(); } </script>
                        </t>
                        </div>
                    <div>
                         <t t-if="error1">
                        <audio id="myAudio1">
                        <source src="scan_invert/static/audio/warning.mp3" type="audio/mp3"/>
                        </audio>
                        <div id="error_audio">
                        <h3 t-esc="error1" class="text-error" style="color:red"/>
                        </div>
                        <script> var divs = document.getElementById("error_audio"); if (divs) { document.getElementById("myAudio1").play(); } </script>
                        </t>
                        </div>
                    </center>
                </div>
            </div>

<!--        </t>-->
    </template>
</odoo>