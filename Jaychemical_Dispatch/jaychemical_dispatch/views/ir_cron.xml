<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="create_product_using_api" model="ir.cron">
            <field name="name">Create Product</field>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_product_template"/>
            <field name="state">code</field>
            <field name="code">model.create_product()</field>
        </record>

        <record id="create_receipt_data" model="ir.cron">
            <field name="name">Create GRN</field>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_stock_picking"/>
            <field name="state">code</field>
            <field name="code">model.create_receipt()</field>
        </record>



