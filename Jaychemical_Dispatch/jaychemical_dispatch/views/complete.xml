<odoo>
    <template id="complete_page" name="Inward">
        <style>
         h1 {
    font-family: "Times New Roman", Times, serif;
    text-shadow: 2px 2px 3px black;
    }
     .oe_structure {
    background-image: url("https://i.pinimg.com/750x/b4/da/b6/b4dab6c409217becb93bd71dc5311f10.jpg");
    height: 100%;
    background-repeat: no-repeat;
    background-size:cover;
    }
        </style>

        <t t-call="website.layout">
            <div class="oe_structure">
                <div class="container">
                    <center>
                        <br />
                        <br />
                        <h1 style="color:green;">Scan Completed</h1>
                        <form action="/scan_complete" method="post" autocomplete="off">
                            <div class="form-group row">
                                <div class="col-lg-12 col-md-12 text-center">
                                    <input type="submit" class="btn btn-outline-primary btn-lg" value="Done" />
                                </div>
                            </div>
                        </form>
                    </center>
                </div>
            </div>
        </t>
    </template>
</odoo>